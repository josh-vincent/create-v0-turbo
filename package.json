{
  "name": "create-v0-turbo",
  "private": true,
  "workspaces": ["apps/*", "packages/*", "packages/features/*", "tooling/*"],
  "engines": {
    "node": ">=22.19.0"
  },
  "packageManager": "bun@1.2.21",
  "scripts": {
    "build": "turbo run build",
    "clean": "git clean -xdf node_modules",
    "clean:workspaces": "turbo run clean",
    "db:push": "turbo -F @tocld/db push",
    "db:studio": "turbo -F @tocld/db studio",
    "dev": "turbo watch dev --continue",
    "dev:next": "turbo watch dev -F @tocld/nextjs...",
    "format": "turbo run format --continue",
    "lint": "turbo run lint --continue",
    "typecheck": "turbo run typecheck",
    "ui-add": "turbo run ui-add",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-aspect-ratio": "^1.1.7",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-context-menu": "^2.2.16",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-menubar": "^1.1.16",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-toggle-group": "^1.1.11",
    "@turbo/gen": "^2.5.6",
    "date-fns": "^4.1.0",
    "react-day-picker": "^9.11.0",
    "react-resizable-panels": "^3.0.6",
    "sonner": "^2.0.7",
    "turbo": "^2.5.6",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@elevenlabs/react": "^0.7.1",
    "streamdown": "^1.3.0",
    "use-stick-to-bottom": "^1.1.1"
  }
}
